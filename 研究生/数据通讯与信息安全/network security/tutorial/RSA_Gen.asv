function [N,d,e] = RSA_Gen(p,q)
    % N= p * q  both p and q is the prime factor of the N
    N=p*q;
    % z is the relatively prime 
    z=(p-1)*(q-1);
    % find the suitable
    for e=N-2:-1:2
        if(gcd(e,z))
            break;
        end
    end
    disp(e);
    for d=1:1:z
        if(mod(e*d,z)==1)
            break;
        end
    end
    disp(d);
end

